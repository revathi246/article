<h1>Edit article</h1>
<%= form_with(model: @article, local: true) do |f| %>
<p>
	<%=f.label :title %><br>
	<%=f.text_field :title %>
</p>
<p>
	<%=f.label :description %><br>
	<%=f.text_area :description %>
</p>

<p>
	<%=f.submit %>
</p>


<% end %> 
<%=link_to 'Back to articles',articles_path%>
